
<div class="contents">

	<div class="title">

		<h2>クチコミ一覧   　<%= @posts_count %>件</h2>

	</div>

	<div class="post-index-list">

		<% @posts.each do |post| %>

		    <!----------------------- 口コミ一覧 ---------------------->
			<%= render partial: 'share/post_index', locals: { post: post } %>

		<% end %>

	</div>


	<!-- ページネーション -->
	<div class="paginate">

	    <%= paginate @posts %>

	</div>


</div>

